{
  "manifest_version": 3,
  "name": "Quick Define & AI",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "js": ["Content.js",
             "./dictionaries/test.js"],
      "matches": ["<all_urls>"],
      "type": "module"
    }
  ],
  "background": { "service_worker": "background.js", "type": "module" },
  "web_accessible_resources": [ 
    { 
      "resources": ["meta/english.txt"], 
      "matches": ["<all_urls>"] 
    }
    ]
}